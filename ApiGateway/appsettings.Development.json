{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Information",
      "Ocelot": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Consul": {
    "Client": {
      "ServiceName": "ApiGateWay",
      "Host": "localhost",
      "Port": 8500
    },
    "Discovery": {
      "Register": true,
      "Deregister": true, 
      "Heartbeat": true, 
      "HealthCheckInterval": "10s",
      "HealthCheckPath": "/health" 
    }
  }
}
